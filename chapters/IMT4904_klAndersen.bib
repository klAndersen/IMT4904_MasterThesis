@Article{Slowiaczek1992,
  Title                    = {Information selection and use in hypothesis testing: What is a good question, and what is a good answer?},
  Author                   = {Slowiaczek, Louisa M. and Klayman, Joshua and Sherman, Steven J. and Skov, Richard B.},
  Journal                  = {Memory {\&} Cognition},
  Year                     = {1992},
  Number                   = {4},
  Pages                    = {392--405},
  Volume                   = {20},

  Abstract                 = {The process of hypothesis testing entails both information selection (asking questions) and information use (drawing inferences from the answers to those questions). We demonstrate that although subjects may be sensitive to diagnosticity in choosing which questions to ask, they are insufficiently sensitive to the fact that different answers to the- same question can have very different diagnosticities. This can lead subjects to overestimate or underestimate the information in the answers they receive. This phenomenon is demonstrated in two experiments using different kinds of inferences (category membership of individuals and composition of sampled populations). In combination with certain information-gathering tendencies, demonstrated in a third experiment, insensitivity to answer diagnosticity can contribute to a tendency toward preservation of the initial hypothesis. Results such as these illustrate the importance of viewing hypothesistesting behavior as an interactive, multistage process that includes selecting questions, interpreting data, and drawing inferences.},
  Doi                      = {10.3758/BF03210923},
  ISSN                     = {1532-5946},
  Url                      = {http://dx.doi.org/10.3758/BF03210923}
}

